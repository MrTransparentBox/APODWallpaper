 <Application x:Class="ConfiguratorGUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ConfiguratorGUI"
             StartupUri="/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>    
            <ResourceDictionary.MergedDictionaries>
                <!--<ResourceDictionary Source="/Styles/Dark.xaml"/>-->
                <ResourceDictionary x:Name="ThemeDict" Source="/Styles/Light.xaml"/>
                <ResourceDictionary Source="/Styles/Templates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
